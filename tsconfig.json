{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "esModuleInterop": true,
    "strict": true,
    "outDir": "./dist",
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [ "src/*" ]
    }
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules","dist"],
}
